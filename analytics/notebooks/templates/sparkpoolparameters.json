{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "nodeCount": {
            "value": 3
        },
        "isComputeIsolationEnabled": {
            "value": false
        },
        "nodeSizeFamily": {
            "value": "MemoryOptimized"
        },
        "nodeSize": {
            "value": "Small"
        },
        "autoScaleEnabled": {
            "value": false
        },
        "autoScaleMinNodeCount": {
            "value": 3
        },
        "autoScaleMaxNodeCount": {
            "value": 10
        },
        "cacheSize": {
            "value": 0
        },
        "dynamicExecutorAllocationEnabled": {
            "value": false
        },
        "autoPauseEnabled": {
            "value": true
        },
        "autoPauseDelayInMinutes": {
            "value": 15
        },
        "sparkVersion": {
            "value": "2.4"
        },
        "sessionLevelPackagesEnabled": {
            "value": false
        },
        "workspaceName": {
            "value": "<azureSynapseWorkspaceName>"
        },
        "location": {
            "value": "<region>"
        },
        "name": {
            "value": "<apacheSparkPoolName>"
        },
        "tags": {
            "value": {}
        }
    }
}